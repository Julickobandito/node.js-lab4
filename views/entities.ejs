<!doctype html>
<html lang="en">
<head>
    <%- include('head'); %>
    <title>Notes</title>
</head>
    <body>
        <%- include('header'); %>
        <section>
            <div class="flex">
                <h1 class="section-title"><%= typeName %>s</h1>
                <div class="flex btns">
                    <div id="paginator" class="arrows">
                        <a id="prev_but" href="#"><img class="arrow" src="/images/prev.png" alt="prev"></a>
                        <div> </div>
                        <a id="next_but" href="#"><img class="arrow" src="/images/next.png" alt="next"></a>
                    </div>
                    <a href="/entities"><img class="home-btn" src="/images/home.png" alt="share"></a>
                    <a id="create-button" class="create-button" href="#">
                        <img class="create-img" src="/images/writing.png" alt="create">
                    </a>
                </div>
            </div>

            <div id="entities-list" class="flex"></div>

            <div id="entity-editor" class="note note-form">
                <div id="inputs"></div>
                <div class="flex">
                    <button id="apply" class="changes">Apply</button>
                    <button id="reset" class="changes">Reset</button>
                    <button id="delete" class="changes">Delete</button>
                </div>
            </div>

        </section>

        <script src="/js/entities.js"></script>
    </body>
</html>
